<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <!-- The manifest file based on emu-35-6-release -->
  <include name="pico/emu-35-6-release.xml" />
  <remove-project name="platform/external/qemu" />
  <remove-project name="platform/hardware/google/aemu" />
  <remove-project name="platform/hardware/google/gfxstream" />
  <remove-project name="platform/prebuilts/android-emulator-build/common" />

  <!-- Pull the modified repositories from the pico branch -->
  <remote alias="aosp" fetch="ssh://stone-review/pico" name="origin" />
  <project name="external/qemu" remote="origin" revision="feature/emu-35-rom-opensource">
    <copyfile src="android/vscode/emu.code-workspace" dest="emu.code-workspace" />
  </project>
  <project name="hardware/google/aemu" remote="origin" revision="feature/emu-35-rom-opensource" />
  <project name="hardware/google/gfxstream" remote="origin" revision="feature/emu-35-rom-opensource" />
  <project name="prebuilts/android-emulator-build/common" remote="origin" revision="feature/emu-35-rom-opensource" />
</manifest>
